#include <stdio.h>

int main(){
	// it want minimum cost 
	// miser
	int T;
	scanf("%d",&T);
	for(int i=0;i<T;i++){
		int g,p;
		scanf("%d %d",&g,&p);
		int n;
		scanf("%d",&n);
		int c1=0,c2=0;
		int a,b;
		for(int k=0;k<n;k++){
			scanf("%d %d",&a,&b);
			if(a==1) c1++;
			if(b==1) c2++;
		}
		int cost1=c1*g + c2*p;
		int cost2=c1*p + c2*g;
		if(cost1<cost2) printf("%d\n",cost1);
		else printf("%d\n",cost2);
	}

}
