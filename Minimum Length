#include <stdio.h>

int main(){
	int T;
    scanf("%d",&T); // number of the test cases
    while(T--){
        int N;
     scanf("%d",&N); // number of the elements in the array
     int A[N],B[N];
        for(int i=0;i<N;i++){
            scanf("%d",&A[i]);
        }
        for(int i=0;i<N;i++){
            scanf("%d",&B[i]);
        }
        // finding the mismatch of the numbers 
        // finding the mismatch from the right
        int ri=-1,le=-1;
        for(int i=N-1;i>=0;i--){
            if(A[i]!=B[i]){
                ri=i;
                break;
            }
        }
            // finding the mismatch from the left
            for(int i=0;i<N;i++){
            if(A[i]!=B[i]){
                le=i;
                break;
            }
        }
        if(le==-1) printf("0\n");
        else
        printf("%d\n",ri-le+1);

    }
}
