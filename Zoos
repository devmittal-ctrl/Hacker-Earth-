#include <stdio.h>

int main(){
	// feel generated
	// first line must start with z
	char str[20];
	scanf("%s",str);
	if(str[0]=='z'){
		int count_z=0,count_o=0;
		for(int i=0;i<20;i++){
			if(str[i]=='z') count_z++;
			if(str[i]=='o') count_o++;
		}
		if(count_z*2==count_o) printf("Yes");
		else printf("No");
	}
}
