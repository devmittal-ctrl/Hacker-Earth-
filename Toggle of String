#include<stdio.h>
int main(){
char str[100];
	scanf("%s",str);
	for(int i=0;str[i]!='\0';i++){
		if(str[i]>=65 && str[i]<=90){
			str[i]=str[i]+32; // upper case to lower case conversion
		}
		if(str[i]>=97 && str[i]<=122){
			str[i]=str[i]-32; // lower case to upper case conversion
		}
	}
	scanf("%s",str);
}
