#include <stdio.h>

int main(){
	 int T;
    scanf("%d", &T);
    while(T--) {
        int N, K;
        scanf("%d %d", &N, &K);
        int A[N], B[N], gain[N];
        long long sum = 0; //avoidig the oveflow
        for(int i = 0; i < N; i++) {
            scanf("%d",&A[i]);
            sum += A[i];
        }
        for(int i = 0; i < N; i++) {
            scanf("%d", &B[i]);
            gain[i] = B[i]-A[i];
        }
        for(int i = 0; i < N; i++) {
            if(gain[i] > 0)
                sum += gain[i];
        }
        printf("%lld\n", sum);
    }
}
